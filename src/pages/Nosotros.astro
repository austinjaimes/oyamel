import InfoHistoria from '../components/InfoHistoria.astro';
import InfoMisionVision from '../components/InfoMisionVision.astro';
---

<!DOCTYPE html>
<html lang="es">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Quienes Somos</title>
    <link href="https://cdn.jsdelivr.net/npm/tailwindcss@2.2.19/dist/tailwind.min.css" rel="stylesheet" />
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Comic+Neue:ital,wght@0,300;0,400;0,700;1,300;1,400;1,700&display=swap" rel="stylesheet">
</head>
<body>
    <Header />
    <ImageTitle imageUrl="/images/quienes-somos.jpg" />
    <InfoQuienesSomos /> 
    <ImageTitle imageUrl="/images/historia.jpg" title="" />
    <InfoHistoria />
    <ImageTitle imageUrl="/images/mision-vision.jpg" title="" />
    <InfoMisionVision />
    <Footer />
